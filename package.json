{
  "name": "ctor-mono",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server": "nodemon --exec ts-node -- index.ts",
    "client": "cd client && npm run start",
    "dev": "concurrently --kill-others-on-fail \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
